        <tr  >
            <th>Name</th>
            <th>City</th>
            <th>fees</th>
            <th>phone</th>
        </tr>
         
        <% Data.map((key)=>{
            %>
            <tr align="center" >
                 <td> <%=key.name %></td>
                 <td> <%=key.city %></td>
                 <td> <%=key.fees %></td>
                 <td> <%=key.phone %></td>
            </tr>
       <% }) %>
     </table>
</body>
